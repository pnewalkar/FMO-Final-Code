<?xml version="1.0" encoding="utf-8" ?>
<ArrayOfCustomFolderSettings xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns:xsd="http://www.w3.org/2001/XMLSchema">
  <CustomFolderSettings FolderID="ExamplePAF_files">
    <FolderEnabled>true</FolderEnabled>
    <FolderDescription>
      Files in format PAF corresponding to the example project
    </FolderDescription>
    <FolderFilter>*.zip</FolderFilter>
    <FolderPath>D:\Project\SourceFiles\ConsoleApplication1\TestData\PAF\</FolderPath>
    <FolderIncludeSub>false</FolderIncludeSub>
    <ExecutableFile>CMD.EXE</ExecutableFile>
    <!-- The block {0} will be automatically replaced with the
      corresponding file name -->
    <ExecutableArguments>PAF</ExecutableArguments>
  </CustomFolderSettings>
  <CustomFolderSettings FolderID="ExampleNYB_files">
    <FolderEnabled>true</FolderEnabled>
    <FolderDescription>
      Files in format NYB corresponding to the example project
    </FolderDescription>
    <FolderFilter>*</FolderFilter>
    <FolderPath>D:\NYB\</FolderPath>
    <FolderIncludeSub>false</FolderIncludeSub>
    <ExecutableFile>CMD.EXE</ExecutableFile>
    <!-- The block {0} will be automatically replaced with the
      corresponding file name -->
    <ExecutableArguments>NYB</ExecutableArguments>
  </CustomFolderSettings>
  <CustomFolderSettings FolderID="ExampleTPF_files">
    <FolderEnabled>true</FolderEnabled>
    <FolderDescription>
      Files in format TPF corresponding to the example project
    </FolderDescription>
    <FolderFilter>*</FolderFilter>
    <FolderPath>D:\Projects\SourceFiles\TPF\</FolderPath>
    <FolderIncludeSub>false</FolderIncludeSub>
    <ExecutableFile>CMD.EXE</ExecutableFile>
    <!-- The block {0} will be automatically replaced with the
      corresponding file name -->
    <ExecutableArguments>USR</ExecutableArguments>
  </CustomFolderSettings>
</ArrayOfCustomFolderSettings>