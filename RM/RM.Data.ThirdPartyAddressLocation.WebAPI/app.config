  <runtime>
    <gcServer enabled="true" />
  </runtime>
  <connectionStrings>
    <add name="RMDBContext"
     connectionString="data source=.\sqlexpress;initial catalog=FMO_Dev01_006;integrated security=True;MultipleActiveResultSets=True;App=EntityFramework" providerName="System.Data.SqlClient" />
  </connectionStrings>
</configuration>