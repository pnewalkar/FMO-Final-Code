    <add key="DeliveryPointExists" value="/DeliveryPointManager/deliverypoint/existence/{0}" />
    <add key="GetDeliveryPointByUDPRNForThirdParty" value="/DeliveryPointManager/deliverypoint/batch/{0}" />
    <add key="GetReferenceDataId" value="/ReferenceDataManager/simpleLists?listName={0}" />
    <add key="UpdateDeliveryPointLocationOnUDPRN" value="/DeliveryPointManager/deliverypoint/batch/location/" />
    <add key="CheckIfNotificationExists" value="/NotificationManager/Notifications/check/{0}/{1}" />
    <add key="DeleteNotificationbyUDPRNAndAction" value="/NotificationManager/Notifications/delete/{0}/{1}" />
    <add key="AddNewNotification" value="/NotificationManager/Notifications/add/" />
    <add key="GetPostCodeSectorByUDPRN" value="/UnitManager/postcodesector/udprn: {0}" />
    <add key="GetPostalAddress" value="/PostalAddressManager/postaladdress/filter/udprn:{0}" />
