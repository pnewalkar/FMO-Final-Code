{
    "env": {
        "browser": true
    },
	
	
    "extends": "eslint:recommended",
	"extends": "plugin:angular/johnpapa",
    "rules": {
	"no-console": "off",
	"eqeqeq" : "error",
        "indent": [
            "error",
            2
        ],
        "linebreak-style": [
            "error",
            "unix"
        ],
        "quotes": [
            "error",
            "single"
        ],
        "semi": [
            "error",
            "always"
        ]
    }
}